
<div class="card shadow">
	<!-- Card Header - Dropdown -->
	<div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
		<h6 class="m-0 font-weight-bold text-primary">@Model.title</h6>
	</div>
	<!-- Card Body -->
	<div class="card-body">
		<div class="chart-area">
			<div class="row">
				<table>
					<tbody>
						<tr>
							<td>
								<select class="form-control inputstl form-user  bg-light border-0 small" placeholder="Expiry">
									<option>Strike</option>
									<option>15000</option>
									<option>15100</option>
									<option>15200</option>
									<option>15300</option>
								</select>
							</td>
							<td>
								<div class="input-group-append"><button class="btn form-user btn-success" type="button">Submit</button></div>
							</td>
						</tr>
					</tbody>
				</table>
			</div>

			<span class="sparkline">Loading...</span>
		
		</div>
	</div>
</div>






